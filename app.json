{
  "expo": {
    "name": "NineT",
    "slug": "ninet-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ninet",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.ninet.newsapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification",
          "background-fetch",
          "remote-notification",
          "background-fetch",
          "remote-notification"
        ],
        "NSFaceIDUsageDescription": "Use Face ID to securely access your personalized news feed",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "ninet-auth",
            "CFBundleURLSchemes": [
              "ninet"
            ]
          },
          {
            "CFBundleURLName": "ninet-auth",
            "CFBundleURLSchemes": [
              "ninet"
            ]
          },
          {
            "CFBundleURLName": "ninet-auth",
            "CFBundleURLSchemes": [
              "ninet"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "io.ninet.newsapp",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "VIBRATE",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "NOTIFICATIONS",
        "USE_FINGERPRINT",
