{
  "expo": {
    "name": "NineT",
    "slug": "ninet-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ninet",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.ninet.newsapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["background-fetch", "remote-notification"],
        "NSFaceIDUsageDescription": "Use Face ID to securely access your personalized news feed"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "io.ninet.newsapp",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-local-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-will-be-auto-generated"
      },
      "apiUrl": "https://ai-news-api.skaybotlabs.workers.dev",
      "supabaseUrl": "your_supabase_url_here",
      "supabaseAnonKey": "your_supabase_anon_key_here"
    }
  }
}